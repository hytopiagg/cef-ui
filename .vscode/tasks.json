{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "cef-build-debug",
            "type": "cargo",
            "command": "run",
            "args": [
                "--bin",
                "cef-build",
                "--release"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            }
        },
        {
            "label": "cef-build-release",
            "type": "cargo",
            "command": "run",
            "args": [
                "--bin",
                "cef-build",
                "--release",
                "--",
                "--release"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            }
        }
    ]
}